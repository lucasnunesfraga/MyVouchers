<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lucasnfraga.myvouchers">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.FormUserActivity" />
        <activity android:name=".activity.VoucherActivity" />
        <activity android:name=".activity.FormEmpresaActivity" />
        <activity android:name=".activity.HomeEmpresaActivity" />
        <activity android:name=".activity.FormVoucherActivity" />
        <activity android:name=".activity.CriarContaActivity"></activity>
    </application>

</manifest>